CREATE TABLE PESERTA(
    ID_PESERTA VARCHAR2(6) NOT NULL,
  NAMA VARCHAR2(20) NOT NULL,
  JURUSAN VARCHAR2(30) NOT NULL,
  TOTAL_SKS  NUMBER NOT NULL,
  PRIMARY KEY(ID_PESERTA)
 );

