 --CREATE TABLE PELAJARAN
CREATE TABLE PELAJARAN(
   KODE_P VARCHAR2(8) NOT NULL,
   MATA_PELAJARAN VARCHAR2(30) NOT NULL,
   JURUSAN VARCHAR2(20) NOT NULL,
   SKS NUMBER NOT NULL,
   PRIMARY KEY(KODE_P)
 )

